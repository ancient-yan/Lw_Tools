<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appwoo.txtw.theme.deepblack"
    android:sharedUserId="android.uid.system"
    android:versionCode="2"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />
    
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
 

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:manageSpaceActivity="com.appwoo.txtw.theme.deepblack.MainActivity"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service android:name=".GetTopService"
            	 android:exported="true" >
        </service>
        
        <service android:name=".BackKeyService"
            	 android:exported="true" >
        </service>
        
        <provider android:name="dbProvider"
            android:exported="true"
            android:authorities="com.txtw.provider.scan.question.DELL"
         />
 
    </application>

</manifest>
